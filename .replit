modules = ["nodejs-20", "web"]
run = "npm run dev"
hidden = [".next", ".git", "node_modules"]

[nix]
channel = "stable-24_05"

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]

[[ports]]
localPort = 5000
externalPort = 80

[env]
PORT = "5000"

[agent]
expertMode = true

[userenv]

[userenv.shared]
NEXTAUTH_URL = "https://0.0.0.0:5000"
ADMIN_EMAILS = "admin@businessbldrs.com"
